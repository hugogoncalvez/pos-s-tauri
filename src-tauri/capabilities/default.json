{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Permisos base del sistema POS",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "core:window:default",
    "core:app:default",
    "core:event:default",
    "core:path:default",
    "core:webview:default",
    "core:menu:default",
    "core:tray:default",
    "core:image:default",
    "core:resources:default",
    "process:default",
    "fs:default",
    "dialog:default",
    "shell:default",
    "log:default",
    "http:allow-fetch",
    "http:allow-fetch-cancel",
    "http:allow-fetch-read-body",
    "http:allow-fetch-send",
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "http://192.168.100.*:8000/*"
        },
        {
          "url": "http://localhost:8000/*"
        },
        {
          "url": "http://127.0.0.1:8000/*"
        },
        {
          "url": "http://detectportal.firefox.com/success.txt"
        }
      ]
    }
  ]
}
